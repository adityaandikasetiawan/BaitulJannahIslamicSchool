    <!-- Page Specific JavaScript -->
    <script>
        // Initialize AOS
        AOS.init({
            duration: 1000,
            easing: 'ease-in-out',
            once: true,
            mirror: false
        });
        
        // Page loading animation
        $(document).ready(function() {
            $('body').addClass('loaded');
        });
        
        // Objective items hover effect
        $('.objective-item').hover(
            function() {
                $(this).addClass('hovered');
            },
            function() {
                $(this).removeClass('hovered');
            }
        );
        
        // Category cards hover effect
        $('.category-card').hover(
            function() {
                $(this).addClass('hovered');
            },
            function() {
                $(this).removeClass('hovered');
            }
        );
        
        // CTA button interaction
        function showRegistrationInfo() {
            alert('Untuk informasi pendaftaran ekstrakurikuler, silakan hubungi bagian kesiswaan atau kunjungi kantor sekolah.');
        }
        
        // Smooth scrolling for internal links
        $('a[href^="#"]').on('click', function(event) {
            var target = $(this.getAttribute('href'));
            if( target.length ) {
                event.preventDefault();
                $('html, body').stop().animate({
                    scrollTop: target.offset().top - 100
                }, 1000);
            }
        });
    </script>
    
    <!-- Custom CSS -->
    <style>
        /* Loading Animation */
        body {
            opacity: 0;
            transition: opacity 0.5s ease-in-out;
        }
        
        body.loaded {
            opacity: 1;
        }
        
        /* Breadcrumb Enhancements */
rna         .breadcrumb-modern {
            background: #ffffff;
            color: #333333;
            padding: 100px 0;
            position: relative;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0,0,0,0.05);
        }
        
        .breadcrumb-modern::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: url('/assets/img/shapes/pattern-1.png');
            opacity: 0.1;
            z-index: 1;
        }
        
        .breadcrumb-modern .container {
            position: relative;
            z-index: 2;
        }
        
        .breadcrumb-subtitle {
            font-size: 1.2rem;
            margin-bottom: 25px;
            opacity: 0.95;
            font-weight: 300;
            letter-spacing: 1px;
            text-transform: uppercase;
            position: relative;
            display: inline-block;
            color: #333333;
        }
        
        .breadcrumb-subtitle::after {
            content: '';
            position: absolute;
            bottom: -8px;
            left: 50%;
            transform: translateX(-50%);
            width: 80px;
            height: 2px;
            background: rgba(178, 31, 31, 0.6);
        }
        
        .breadcrumb-icon {
            position: relative;
            animation: float 3s ease-in-out infinite;
        }
        
        .breadcrumb-icon::after {
            content: '';
            position: absolute;
            bottom: -15px;
            left: 50%;
            transform: translateX(-50%);
            width: 50px;
            height: 15px;
            background: radial-gradient(ellipse at center, rgba(255,255,255,0.3) 0%, rgba(255,255,255,0) 70%);
            border-radius: 50%;
        }
        
        @keyframes float {
            0% {
                transform: translateY(0px);
            }
            50% {
                transform: translateY(-15px);
            }
            100% {
                transform: translateY(0px);
            }
        }
        
        /* Overview Card */
        .overview-card {
            border: none;
            box-shadow: 0 20px 50px rgba(0,0,0,0.15);
            border-radius: 20px;
            padding: 60px 30px;
            background: rgba(255, 255, 255, 0.9);
            backdrop-filter: blur(10px);
            -webkit-backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.3);
            position: relative;
            overflow: hidden;
            margin-top: -50px;
            z-index: 10;
            transition: all 0.5s cubic-bezier(0.165, 0.84, 0.44, 1);
        }
        
        .overview-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 30px 60px rgba(0, 0, 0, 0.15), 0 10px 25px rgba(0, 0, 0, 0.1);
            border-color: rgba(26, 42, 108, 0.2);
        }
        
        .overview-icon {
            margin-bottom: 30px;
            position: relative;
        }
        
        .overview-icon i {
            font-size: 5rem;
            background: linear-gradient(135deg, #f6d365 0%, #fda085 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            display: inline-block;
            position: relative;
            filter: drop-shadow(0 2px 4px rgba(0,0,0,0.2));
            transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
            cursor: pointer;
        }
        
        .overview-icon:hover i {
            transform: scale(1.1) rotate(5deg);
            background: linear-gradient(135deg, #fdbb2d 30%, #b21f1f 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            filter: drop-shadow(0 0 8px rgba(253, 160, 133, 0.5));
            animation: shimmer 2s ease-in-out infinite;
        }
        
        @keyframes shimmer {
            0% { filter: drop-shadow(0 0 5px rgba(253, 160, 133, 0.3)); }
            50% { filter: drop-shadow(0 0 12px rgba(253, 160, 133, 0.6)); }
            100% { filter: drop-shadow(0 0 5px rgba(253, 160, 133, 0.3)); }
        }
        
        .overview-icon:hover .overview-icon-decoration {
            animation-duration: 15s;
            border-color: rgba(253, 160, 133, 0.5);
            box-shadow: 0 0 15px rgba(253, 160, 133, 0.2);
        }
        
        .overview-icon:hover .overview-icon-decoration::before {
            border-color: rgba(26, 42, 108, 0.4);
            animation-duration: 10s;
            width: 90px;
            height: 90px;
        }
        
        .overview-icon:hover .overview-icon-decoration::after {
            border-color: rgba(178, 31, 31, 0.3);
            animation-duration: 2s;
            width: 170px;
            height: 170px;
        }
        
        .objective-icon::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(135deg, rgba(178, 31, 31, 0.1) 0%, rgba(253, 160, 133, 0.1) 100%);
            z-index: -1;
            transition: all 0.5s ease;
        }
        
        .overview-card:hover::before {
            background: linear-gradient(135deg, rgba(178, 31, 31, 0.05) 0%, rgba(253, 160, 133, 0.05) 100%);
            opacity: 1;
        }
        
        .objective-icon::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(135deg, rgba(178, 31, 31, 0.03) 0%, rgba(253, 160, 133, 0.03) 100%);
            opacity: 0;
            transition: all 0.5s ease;
            z-index: -1;
        }
        
        .category-header {
            background: #ffffff;
            color: #333333;
            padding: 40px 25px;
            text-align: center;
            position: relative;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0,0,0,0.05);
