<%- include('partials/header2') %>

			<!-- Breadcrumb -->
			<div class="breadcrumb-bar text-center">
				<div class="container">
					<div class="row">
						<div class="col-md-12 col-12">
							<h2 class="breadcrumb-title mb-2">Galeri</h2>
							<nav aria-label="breadcrumb">
								<ol class="breadcrumb justify-content-center mb-0">
									<li class="breadcrumb-item"><a href="/">Home</a></li>
									<li class="breadcrumb-item">Informasi</li>
									<li class="breadcrumb-item active" aria-current="page">Galeri</li>
								</ol>
							</nav>
						</div>
					</div>
				</div>
			</div>
			<!-- /Breadcrumb -->

            <!-- Galeri Content -->
            <div class="content">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="section-header text-center mb-5">
                                <div class="section-line"></div>
                                <h2 class="section-title">Galeri Kegiatan</h2>
                                <p>Dokumentasi foto dan video kegiatan Baitul Jannah Islamic School</p>
                            </div>
                        </div>
                    </div>

                    <!-- Filter Kategori -->
                    <div class="row">
                        <div class="col-lg-12 mb-4">
                            <div class="gallery-filter d-flex justify-content-center flex-wrap">
                                <button class="btn btn-outline-primary active me-2 mb-2" data-filter="*">Semua</button>
                                <button class="btn btn-outline-primary me-2 mb-2" data-filter=".akademik">Akademik</button>
                                <button class="btn btn-outline-primary me-2 mb-2" data-filter=".keagamaan">Keagamaan</button>
                                <button class="btn btn-outline-primary me-2 mb-2" data-filter=".ekstrakurikuler">Ekstrakurikuler</button>
                                <button class="btn btn-outline-primary me-2 mb-2" data-filter=".wisuda">Wisuda</button>
                                <button class="btn btn-outline-primary mb-2" data-filter=".video">Video</button>
                            </div>
                        </div>
                    </div>

                    <!-- Galeri Items -->
                    <div class="row gallery-container">
                        <!-- Galeri Item 1 - Akademik -->
                        <div class="col-md-6 col-lg-4 mb-4 gallery-item akademik">
                            <div class="gallery-box">
                                <div class="gallery-img">
                                    <a href="assets/img/gallery/gallery-large-01.jpg" data-lightbox="gallery" data-title="Kegiatan Pembelajaran Aktif di Kelas">
                                        <img src="assets/img/gallery/gallery-01.jpg" class="img-fluid" alt="Kegiatan Pembelajaran">
                                        <div class="gallery-overlay">
                                            <i class="fas fa-search-plus"></i>
                                        </div>
                                    </a>
                                </div>
                                <div class="gallery-info">
                                    <h5>Kegiatan Pembelajaran Aktif di Kelas</h5>
                                    <p><i class="fas fa-calendar-alt me-1"></i> 15 Januari 2024</p>
                                    <span class="badge bg-primary">Akademik</span>
                                </div>
                            </div>
                        </div>

                        <!-- Galeri Item 2 - Keagamaan -->
                        <div class="col-md-6 col-lg-4 mb-4 gallery-item keagamaan">
                            <div class="gallery-box">
                                <div class="gallery-img">
                                    <a href="assets/img/gallery/gallery-large-02.jpg" data-lightbox="gallery" data-title="Program Tahfidz Al-Qur'an">
                                        <img src="assets/img/gallery/gallery-02.jpg" class="img-fluid" alt="Program Tahfidz">
                                        <div class="gallery-overlay">
                                            <i class="fas fa-search-plus"></i>
                                        </div>
                                    </a>
                                </div>
                                <div class="gallery-info">
                                    <h5>Program Tahfidz Al-Qur'an</h5>
                                    <p><i class="fas fa-calendar-alt me-1"></i> 20 Februari 2024</p>
                                    <span class="badge bg-success">Keagamaan</span>
                                </div>
                            </div>
                        </div>

                        <!-- Galeri Item 3 - Ekstrakurikuler -->
                        <div class="col-md-6 col-lg-4 mb-4 gallery-item ekstrakurikuler">
                            <div class="gallery-box">
                                <div class="gallery-img">
                                    <a href="assets/img/gallery/gallery-large-03.jpg" data-lightbox="gallery" data-title="Latihan Pramuka">
